{
  "manifest_version": 2,

  "name": "Youtube subs as text",
  "description": "This extension will extract subs of youtube video",
  "version": "1.0",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],
   "content_scripts": [{
    //"css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["<all_urls>"]
  }]
}